{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.4"
        },
        "*.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "/api/webhook.php"
        },
        {
            "src": "/privacy",
            "dest": "/privacy.php"
        },
        {
            "src": "/terms",
            "dest": "/terms.php"
        },
        {
            "src": "/deletion",
            "dest": "/deletion.php"
        }
    ]
}